<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>jsmp导航系统</display-name>

	<servlet>
		<servlet-name>NavServiceGet</servlet-name>
		<servlet-class>jetsennet.jsmp.nav.service.a7.NavServiceGet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NavServiceGet</servlet-name>
		<url-pattern>/NavService</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>NavServicePost</servlet-name>
		<servlet-class>jetsennet.jsmp.nav.service.a7.NavServicePost</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NavServicePost</servlet-name>
		<url-pattern>/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>monitor</servlet-name>
		<servlet-class>jetsennet.jsmp.nav.monitor.MonitorServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
</web-app>
